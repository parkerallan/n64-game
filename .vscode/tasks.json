{
  "version": "2.0.0",
  "tasks": [
      {
        "label": "Build ROM",
        "type": "shell",
        "command": "cd /d/n64game && make",
        "options": {
          "shell": {
            "executable": "C:\\msys64\\usr\\bin\\bash.exe",
            "args": ["-l", "-c"]
          }
        },
        "group": {
            "kind": "build",
            "isDefault": true
        },
        "problemMatcher": ["$gcc"]
      },
      {
        "label": "Clean Build",
        "type": "shell",
        "command": "cd /d/n64game && make clean",
        "options": {
          "shell": {
            "executable": "C:\\msys64\\usr\\bin\\bash.exe",
            "args": ["-l", "-c"]
          }
        },
        "group": {
            "kind": "build",
            "isDefault": true
        },
        "problemMatcher": ["$gcc"]
      },
      {
        "label": "Run ROM",
        "type": "shell",
        "command": "D:/ares/ares.exe D:/n64game/cypher.z64",
        "group": {
            "kind": "test",
            "isDefault": true
        },
        "problemMatcher": []
      }
  ]
}